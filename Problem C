#include<stdio.h>
#include<algorithm>
int main() {
    int c,n;
    while (scanf("%d", &c) != EOF) {
    {
            for (int i = 0; i < c; i++) {
                scanf("%d", &n);
                while (n % 2 == 0) n /= 2;
                while (n % 3 == 0) n /= 3;
                while (n % 5 == 0) n /= 5;
                while (n % 7 == 0) n /= 7;
                if (n == 1)
                    printf("Yes\n");
                else printf("No\n");
            }
        }
        }
    return 0;
}
